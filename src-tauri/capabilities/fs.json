{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "fs",
  "description": "Capability for filesystem access",
  "windows": ["main"],
  "permissions": [
    "path:default",
    "fs:scope:allow-read",
    "fs:scope:allow-write",
    "fs:scope:allow-create-dir",
    "fs:scope:allow-delete",
    "fs:scope:allow-rename",
    "fs:scope:allow-copy",
    "fs:scope:allow-exists",
    "fs:scope:allow-read-dir"
  ]
}